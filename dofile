!main run
!require mkdir cp chmod unzip bash python3
!def run
    set -e
    echo "Building"
    mkdir -p build
    cp do.py build/do
    chmod +x build/do
!def install:admin
    set -e
    echo "Installing"
    cp build/do /usr/bin
    ln -svf /usr/bin/do /usr/bin/sbuild
    mkdir -p /usr/lib/do
    cp template.py /usr/lib/do
!def clean
    rm -rf build
    #Clean unzipped lib folder
!def uninstall:admin
    rm /usr/bin/do
    rm /usr/bin/sbuild
    rm -rf /usr/lib/do
!def argtest
    echo Argtest one
    echo $1
    echo $2
    !execute argtest2 $1 $2
!def argtest2
    echo Argtest 2
    echo "${1}again"
    echo "${2}again"